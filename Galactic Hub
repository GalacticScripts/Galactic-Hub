-- create window, start on Default
local Window =
    Rayfield:CreateWindow(
    {
        Name = "Galactic Scripts Hub",
        LoadingTitle = "Rayfield Interface Suite",
        LoadingSubtitle = "by GalacticScripts",
        Theme = "Default",
        DisableRayfieldPrompts = false,
        DisableBuildWarnings = false,
        Discord = {Enabled = true, Invite = "Utg6QeEzwm", RememberJoins = true},
        KeySystem = false
    }
)

-- Themes tab & dropdown
local themeTab = Window:CreateTab("üé® Themes", 4483362458)
themeTab:CreateSection("Theme Selector")
themeTab:CreateDropdown(
    {
        Name = "Current Theme",
        Options = {
            "Default",
            "AmberGlow",
            "Amethyst",
            "Bloom",
            "DarkBlue",
            "Green",
            "Light",
            "Ocean",
            "Serenity",
            "Blossom",
            "Crimson"
        },
        CurrentOption = {"Default"},
        MultipleOptions = false,
        Callback = function(selection)
            local t = selection[1]
            if t == "Blossom" then
                Window.ModifyTheme(GetBlossomTheme())
            elseif t == "Crimson" then
                Window.ModifyTheme(GetCrimsonTheme())
            else
                Window.ModifyTheme(t)
            end
        end
    }
)

-- ‚îÄ‚îÄ Your updates table ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

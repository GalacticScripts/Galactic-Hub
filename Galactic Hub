        ElementBackground = Color3.fromRGB(20, 20, 20),
        ElementBackgroundHover = Color3.fromRGB(40, 0, 0),
        SecondaryElementBackground = Color3.fromRGB(10, 10, 10),
        ElementStroke = Color3.fromRGB(80, 0, 0),
        SecondaryElementStroke = Color3.fromRGB(40, 0, 0),
        SliderBackground = Color3.fromRGB(255, 0, 0),
        SliderProgress = Color3.fromRGB(255, 0, 0),
        SliderStroke = Color3.fromRGB(255, 80, 80),
        ToggleBackground = Color3.fromRGB(20, 20, 20),
        ToggleEnabled = Color3.fromRGB(255, 0, 0),
        ToggleDisabled = Color3.fromRGB(100, 100, 100),
        ToggleEnabledStroke = Color3.fromRGB(255, 80, 80),
        ToggleDisabledStroke = Color3.fromRGB(125, 125, 125),

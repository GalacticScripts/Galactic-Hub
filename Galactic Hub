    function()
        error("üî• Intentional error test")
    end
)

-- ‚îÄ‚îÄ üè† Home Tab with Live Stats & Latest Updates ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local tabs = {}
tabs["üè† Home"] = Window:CreateTab("üè† Home", 4483362458)
local home = tabs["üè† Home"]

home:CreateSection("Galactic Scripts v1.0.0")
home:CreateParagraph(
    {
        Title = "Welcome!",
        Content = "Hello, " .. Players.LocalPlayer.Name .. "!"
    }
)

home:CreateSection("üìä Your Session")

-- Create a label to show FPS in your Rayfield tab
local FPSLabel = home:CreateLabel("FPS: 0")
local lastTime = tick() -- Store the current time (tick is the number of seconds since the game started)
local frames = 0 -- Initialize frame counter

-- Connect to RenderStepped event to calculate FPS
RunService.RenderStepped:Connect(
    function()
        frames = frames + 1 -- Increment the frame count on every render step
        local now = tick() -- Get the current time
        if now - lastTime >= 1 then -- If a second has passed
            FPSLabel:Set("FPS: " .. frames) -- Update the FPS label
            frames = 0 -- Reset the frame counter
            lastTime = now -- Reset the last time to the current time
        end
    end
)

-- Create Player Count label in the Home Tab
local PlayerCountLabel = home:CreateLabel("Players: 0", nil, nil, false)

-- Update initial player count
PlayerCountLabel:Set("Players: " .. #Players:GetPlayers(), nil, nil, false)

-- Update player count when players join or leave
Players.PlayerAdded:Connect(
    function()
        PlayerCountLabel:Set("Players: " .. #Players:GetPlayers(), nil, nil, false)
    end
)

Players.PlayerRemoving:Connect(
    function()
        PlayerCountLabel:Set("Players: " .. #Players:GetPlayers(), nil, nil, false)
    end
)

home:CreateSection("üì¢ Latest Updates")
for i = math.max(#updates - 1, 1), #updates do
    local u = updates[i]
    home:CreateParagraph(
        {
            Title = u.Title,
            Content = u.Content
        }
    )
end

home:CreateSection("üîç Search Game Tabs")

home:CreateInput(
    {
        Name = "Game Name",
        PlaceholderText = "Type to find a game‚Ä¶",
        RemoveTextAfterFocusLost = false,
        Callback = function(txt)
            for name, tabObj in pairs(tabs) do
                if name:lower():find(txt:lower(), 1, true) then
                    if Window.SetActiveTab then
                        Window:SetActiveTab(tabObj)
                    else
                        Rayfield:Notify(
                            {
                                Title = "Found ‚Äú" .. name .. "‚Äù",
                                Content = "Please click its tab above.",
                                Duration = 3
                            }
                        )
                    end
                    return
                end
            end
            Rayfield:Notify(
                {
                    Title = "Not Found",
                    Content = "No game matches '" .. txt .. "'.",
                    Duration = 3
                }
            )
        end
    }
)

home:CreateSection("Discord")

home:CreateButton(
    {
        Name = "Join Our Discord",
        Callback = function()
            setclipboard("https://discord.gg/Utg6QeEzwm")
            Rayfield:Notify({Title = "Discord", Content = "Link copied!", Duration = 4})
        end
    }
)


    {Title = "v1.0.0 ‚Äì Initial Release", Content = "Galactic Scripts Hub Releases!"}
}

-- ‚îÄ‚îÄ üö® Error-Handler & üñ•Ô∏è Developer Console Setup ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

local consoleTab = Window:CreateTab("üñ•Ô∏è Console", 4483362458)
consoleTab:CreateSection("Developer Console")

local consoleCount = 0
local function devConsoleLog(msg)
    consoleCount = consoleCount + 1
    consoleTab:CreateParagraph(
        {
            Title = "[" .. consoleCount .. "]",
            Content = tostring(msg)
        }
    )
end

-- Override print function to log to the console tab
local _oldPrint = print
print = function(...)
    -- Call original print function
    _oldPrint(...)

    -- Get arguments passed to print()
    local parts = {...}

    -- Join all parts with a tab separator using table.concat
    local line = table.concat(parts, "\t")

    -- Send the output to the dev console log
    devConsoleLog(line)
end

-- hook runtime errors
local ScriptContext = game:GetService("ScriptContext")
ScriptContext.Error:Connect(
    function(message, stackTrace)
        Rayfield:Notify(
            {
                Title = "üö® Script Error",
                Content = message,
                Duration = 8
            }
        )
        devConsoleLog("ERROR: " .. message .. "\n" .. stackTrace)
    end
)

-- clear-console button
consoleTab:CreateButton(
    {
        Name = "üóëÔ∏è Clear Console",
        Callback = function()
            consoleCount = 0
            -- Create a new section and reset paragraphs manually (clear the content)
            consoleTab:CreateSection("Developer Console")
            devConsoleLog("Console Cleared!")
        end
    }
)


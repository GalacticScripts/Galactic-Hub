Content.BackgroundTransparency = 1
Content.Parent = MainFrame

local Button = Instance.new("TextButton")
Button.Size = UDim2.new(1, 0, 1, 0)
Button.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
Button.Text = "ðŸŽ„ Desync"
Button.TextColor3 = Color3.fromRGB(255, 255, 240)
Button.Font = Enum.Font.GothamBold
Button.TextSize = 14
Button.Parent = Content

local ButtonCorner = Instance.new("UICorner")
ButtonCorner.CornerRadius = UDim.new(0, 8)
ButtonCorner.Parent = Button

local ButtonGradient = Instance.new("UIGradient")
ButtonGradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(190, 0, 0)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(150, 0, 0))
})
ButtonGradient.Rotation = 90
ButtonGradient.Parent = Button

local ButtonStroke = Instance.new("UIStroke")
ButtonStroke.Color = Color3.fromRGB(255, 215, 0)
ButtonStroke.Thickness = 1
ButtonStroke.Transparency = 0.4
ButtonStroke.Parent = Button

Button.MouseEnter:Connect(function()
    TweenService:Create(Button, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    }):Play()
    TweenService:Create(ButtonStroke, TweenInfo.new(0.2), {
        Transparency = 0.2
    }):Play()
end)

Button.MouseLeave:Connect(function()
    TweenService:Create(Button, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(170, 0, 0)
    }):Play()
    TweenService:Create(ButtonStroke, TweenInfo.new(0.2), {
        Transparency = 0.4
    }):Play()
end)

local dragging, dragInput, dragStart, startPos
Button.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

Button.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

local function createFloatingOrnament(parent, position, size, color)
    local Ornament = Instance.new("Frame")
    Ornament.Size = size
    Ornament.Position = position
    Ornament.BackgroundColor3 = color
    Ornament.BorderSizePixel = 0
    Ornament.Parent = parent
    
    local Corner = Instance.new("UICorner")
    Corner.CornerRadius = UDim.new(1, 0)
    Corner.Parent = Ornament
    
    local Stroke = Instance.new("UIStroke")
    Stroke.Color = Color3.fromRGB(255, 215, 0)
    Stroke.Thickness = 1
    Stroke.Parent = Ornament
    
    return Ornament
end

local ornaments = {}
local ornamentPositions = {
    {UDim2.new(0, -6, 0, 20), UDim2.new(0, 12, 0, 12)},
    {UDim2.new(1, -8, 0, 30), UDim2.new(0, 10, 0, 10)},
    {UDim2.new(0, -4, 1, -25), UDim2.new(0, 14, 0, 14)},
    {UDim2.new(1, -10, 1, -20), UDim2.new(0, 16, 0, 16)}
}

local ornamentColors = {
    Color3.fromRGB(255, 0, 0),
    Color3.fromRGB(0, 255, 0),
    Color3.fromRGB(255, 255, 0),
    Color3.fromRGB(0, 0, 255)
}

for i, posData in ipairs(ornamentPositions) do
    local ornament = createFloatingOrnament(MainFrame, posData[1], posData[2], ornamentColors[i])
    table.insert(ornaments, ornament)
end

local time = 0
RunService.Heartbeat:Connect(function(delta)
    time = time + delta
    for i, ornament in ipairs(ornaments) do
        local offset = math.sin(time + i) * 2
        local currentPos = ornamentPositions[i][1]
        ornament.Position = UDim2.new(
            currentPos.X.Scale,
            currentPos.X.Offset + offset,
            currentPos.Y.Scale,
            currentPos.Y.Offset
        )
    end
end)


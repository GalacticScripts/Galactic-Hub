local function respawn(plr)
    for name, value in pairs(FFlags) do
        pcall(function()
            setfflag(tostring(name), tostring(value))
        end)
    end
end

Button.MouseButton1Click:Connect(function()
    local char = LocalPlayer.Character
    local root = char and char:FindFirstChild("HumanoidRootPart")

    if root then
        local backCFrame = root.CFrame * CFrame.new(0, 0, 50)
        root.CFrame = backCFrame
    end

    task.wait(0.2)

    respawn(LocalPlayer)

    local Notify = Instance.new("Frame")
    Notify.Size = UDim2.new(0, 200, 0, 40)
    Notify.Position = UDim2.new(0, 20, 1, -60)
    Notify.BackgroundColor3 = Color3.fromRGB(15, 40, 20)
    Notify.BorderSizePixel = 0
    Notify.BackgroundTransparency = 0.1
    Notify.AnchorPoint = Vector2.new(0, 1)
    Notify.Parent = ScreenGui

    local NotifyCorner = Instance.new("UICorner")
    NotifyCorner.CornerRadius = UDim.new(0, 8)
    NotifyCorner.Parent = Notify

    local NotifyGradient = Instance.new("UIGradient")
    NotifyGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(20, 50, 25)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(15, 40, 20))
    })
    NotifyGradient.Rotation = 90
    NotifyGradient.Parent = Notify

    local NotifyStroke = Instance.new("UIStroke")
    NotifyStroke.Color = Color3.fromRGB(255, 215, 0)
    NotifyStroke.Thickness = 1
    NotifyStroke.Parent = Notify

    local NotifyText = Instance.new("TextLabel")
    NotifyText.Size = UDim2.new(1, 0, 1, 0)
    NotifyText.BackgroundTransparency = 1
    NotifyText.Font = Enum.Font.GothamBold
    NotifyText.TextColor3 = Color3.fromRGB(255, 255, 240)
    NotifyText.TextSize = 14
    NotifyText.Text = "ðŸŽ… Desync Activated!"
    NotifyText.Parent = Notify

    TweenService:Create(Notify, TweenInfo.new(0.3), {Position = UDim2.new(0, 20, 1, -75)}):Play()
    task.wait(2)
    TweenService:Create(Notify, TweenInfo.new(0.3), {BackgroundTransparency = 1, Position = UDim2.new(0, 20, 1, 0)}):Play()
    task.wait(0.3)
    Notify:Destroy()
end)
